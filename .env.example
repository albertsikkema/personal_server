# Environment variables for FastAPI application
# Copy this file to .env and update with your actual values

# Required: API Key for authentication (minimum 8 characters)
API_KEY=your-secret-api-key-here

# Optional: Application configuration
# APP_NAME=FastAPI Application
# DEBUG=true
# ENV=development  # Environment: development, staging, production (docs hidden in production)
# LOG_LEVEL=INFO

# Optional: Logging configuration
# LOG_TO_FILE=true
# LOG_FILE_PATH=logs
# LOG_FILE_NAME=fastapi.log
# LOG_MAX_BYTES=10485760
# LOG_BACKUP_COUNT=5
# LOG_JSON_FORMAT=true